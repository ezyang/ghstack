from ghstack.test_prelude import *

init_test()

commit("A")
commit("B")
(
    diff1,
    diff2,
) = gh_submit("Initial")
assert diff1 is not None
assert diff2 is not None
pr_url = diff2.pr_url

gh_land(pr_url)
assert_expected_inline(
    get_upstream_sh().git("log", "--oneline", "master"),
    """\
b2d9b84 Commit B
fe65c88 Commit A
dc8bfe4 Initial commit""",
)
